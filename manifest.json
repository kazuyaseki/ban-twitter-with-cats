{
  "name": "Ban Twitter with Cats",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Ban Twitter with Cats",
  "permissions": [
    "storage"
  ],
  "content_scripts": [{
    "matches": ["https://twitter.com/*"],
    "js": [
      "index.js"
    ]
  }],
  "browser_action":{
    "default_icon": {
      "16": "cat_enabled.png"
    },
    "default_title":"toggle status",
    "default_popup":"browserAction.html"
  },
  "web_accessible_resources": [
    "cat_images/*.jpeg"
  ]
}